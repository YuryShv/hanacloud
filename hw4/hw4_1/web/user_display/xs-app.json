{
  "welcomeFile": "user_display/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination": "pt_service"
    },
    {
      "source": "^/user_display/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "xsuaa"
    }
  ]
}